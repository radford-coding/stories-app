<%- include('../partials/_htmlhead') %>
<title><%= userFound.username %></title>
</head>

<body>
    <%- include('../partials/_navbar') %>
    <h1><%= userFound.username %></h1>
    <ul>
        <% stories.forEach(story => { %>
        <li><a href="/stories/<%= story._id %>"><%= story.name %></a></li>
        <% }); %>
    </ul>
    <% if (me) { %>
        <a href="/users/<%= userFound._id %>/edit">edit my profile</a>
    <% }; %>
    <br><br>
</body>

</html>